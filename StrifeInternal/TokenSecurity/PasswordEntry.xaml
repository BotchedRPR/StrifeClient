<Window x:Class="StrifeClient.StrifeInternal.TokenSecurity.PasswordEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StrifeClient.StrifeInternal.TokenSecurity"
        mc:Ignorable="d"
        Title="PasswordEntry"  SizeToContent="WidthAndHeight"
        Height="111" Width="379">
    <Grid Background="Black">
        <Label Content="Authenticate using your Token password, then click OK." HorizontalContentAlignment="Center" Margin="10,7,10,62" Background="Black" Foreground="White"/>
        <Button Content="OK" Margin="10,65,10,10" Click="Button_Click"/>
        <PasswordBox x:Name="pbox" Margin="10,37,10,37"/>

    </Grid>
</Window>
